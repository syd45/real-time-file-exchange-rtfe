    "express": "^4.18.2",
    "fs-extra": "^11.1.1",
    "helmet": "^7.0.0",
    "path": "^0.12.7",
    "webdav-server": "^2.6.2",
    "ws": "^8.13.0",
    "pm2": "^5.3.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  }
